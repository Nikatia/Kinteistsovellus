@model Kiinteistosovellus.Models.MonthlySpendings




<div class="modal fade" id="ModalMSDelete">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
              
            </div>
            <div class="modal-body">


              

                <h3>Are you sure you want to delete this?</h3>
                <div>
                    <h4>MonthlySpendings</h4>
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            Aloituspäivämäärä
                        </dt>

                        <dd>
                            @if (Model.DateBegin != null)
                            {@Convert.ToDateTime(Model.DateBegin).ToString("dd/MM/yyyy")}
                        @*@Html.DisplayFor(model => model.DateBegin)*@
                        </dd>

                        <dt>
                            Päättymispäivämäärä
                        </dt>

                        <dd>
                            @if (Model.DateBegin != null)
                            {@Convert.ToDateTime(Model.DateBegin).ToString("dd/MM/yyyy")}
                        @*@Html.DisplayFor(model => model.DateEnd)*@
                        </dd>

                        <dt>
                            Yksikkömäärä
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.AmountOfUnits)
                        </dd>

                        <dt>
                            Yksikköhinta
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PricePerUnit)
                        </dd>

                        <dt>
                            Siirtohinta
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.TransferPayment)
                        </dd>

                        <dt>
                            Kokonaishinta
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.FullPrice)
                        </dd>

                        <dt>
                            Yrityksen nimi
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Contractors.Name)
                        </dd>

                        <dt>
                            Käyttäjän nimi
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Logins.UserName)
                        </dd>

                        <dt>
                            Kulutustyyppi
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.MonthlySpendingTypes.TypeName)
                        </dd>

                    </dl>



                </div>
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <div class="modal-footer" style="display:inline-block">
                        <button type="submit" class="btn btn-primary">Delete</button>
                        <a href="@Url.Action("index","MonthlySpendings")" class="btn btn-primary">Back</a>
                    </div>
                }
            </div>
        </div>
    </div>
</div>








