@model Kiinteistosovellus.Models.Posts




<div class="modal fade" id="ModalMSDelete">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">

            </div>
            <div class="modal-body">




                <h3>Oletko varma, että haluat poistaa tämän?</h3>
                <div>
                    <h4>Postinumerot</h4>
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Country)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Country)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.PostCode)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PostCode)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.City)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.City)
                        </dd>

                    </dl>



                </div>
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <div class="modal-footer" style="display:inline-block">
                        <button type="submit" class="btn btn-primary">Poista</button>
                        @Html.ActionLink("Takaisin", "Index", "Monthly_spendings", new { @Class = "btn btn-secondary" })
                    </div>
                }
            </div>
        </div>
    </div>
</div>








